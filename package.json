{
  "version": "0.1.0-alpha.2",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "start": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "@mormahr/postal",
  "author": "Moritz Mahringer",
  "module": "dist/postal.esm.js",
  "size-limit": [
    {
      "path": "dist/postal.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/postal.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "9.0.0",
    "@types/node-fetch": "2.6.7",
    "fetch-mock": "9.11.0",
    "fetch-mock-jest": "1.5.1",
    "husky": "7.0.4",
    "size-limit": "6.0.4",
    "tsdx": "0.14.1",
    "tslib": "2.6.2",
    "typescript": "4.9.5"
  },
  "dependencies": {
    "node-fetch": "^2"
  }
}
